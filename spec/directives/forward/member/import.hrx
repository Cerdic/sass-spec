  b: d;
}

<===>
================================================================================
<===> forward_to_import/with/input.scss
@use "used";

a {b: used.get-c()};

<===> forward_to_import/with/_used.scss
@forward "forwarded" with ($c: new value);

<===> forward_to_import/with/_forwarded.scss
@import "imported";

<===> forward_to_import/with/_imported.scss
$c: old value !default;

@function get-c() {@return $c}

<===> forward_to_import/with/output.css
a {
  b: new value;
}

